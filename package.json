    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.18.6",
    "gatsby-plugin-emotion": "^4.3.2",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-graphql": "^2.5.7",
    "gatsby-source-wordpress": "^3.3.13",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "typescript": "^3.9.2"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
